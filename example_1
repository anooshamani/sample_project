#include<stdio.h>
void fun(int arr[],int n)
{
	int i;
	for(i=0;i<n;i++)
	{
		arr[i]=arr[i]+2;
	}
}
int main()
{
	int n,i;
	printf("enter the element in the array:\n");
	scanf("%d",&n);
	int arr[n];
	for(i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}

	for(i=0;i<n;i++)
	{
		printf("%d ",arr[i]);
	}
	printf("\n");

	fun(arr,n);
	 for(i=0;i<n;i++)
        {
                printf("%d ",arr[i]);
        }
	 printf("\n");




}
